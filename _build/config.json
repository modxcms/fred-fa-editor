{
  "name": "FredFAEditor",
  "lowCaseName": "fredfaeditor",
  "description": "FontAwesome Editor for Fred",
  "author": "John Peca",
  "version": "1.2.0-pl",
  "package": {
    "menus": [],
    "elements": {
      "plugins": [
        {
          "name": "FredFAEditor",
          "file": "FredFAEditor.php",
          "events": [
            "FredBeforeRender"
          ]
        }
      ]
    },
    "systemSettings": [
      {
        "key": "link",
        "value": "https://use.fontawesome.com/releases/v6.6.0/css/all.css"
      }
    ]
  },
  "dependencies": [
    {
      "name": "fred",
      "version": ">=1.1.0"
    }
  ],
  "build": {
    "resolver": {
      "after": [
        "resolve.system_setting.php"
      ]
    }
  }
}
